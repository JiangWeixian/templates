{
  "name": "@aiou/ts-lib-template",
  "version": "0.3.7",
  "description": "a starter for typescript lib",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "url": "https://github.com/JiangWeixian/templates/tree/master/packages/ts-lib-template"
  },
  "keywords": [
    "typescript",
    "lib",
    "template",
    "aiou"
  ],
  "watch": {
    "run_dev": {
      "patterns": [
        "src"
      ],
      "extensions": "ts",
      "quiet": true,
      "legacyWatch": true,
      "delay": 1000,
      "runOnChangeOnly": false
    }
  },
  "devDependencies": {
    "@aiou/eslint-config": "0.0.12",
    "@types/jest": "26.0.23",
    "eslint": "^7.30.0",
    "husky": "^3.1.0",
    "jest": "27.0.6",
    "np": "7.5.0",
    "npm-watch": "0.10.0",
    "prettier": "2.3.2",
    "pretty-quick": "3.1.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.3",
    "ts-node": "10.0.0",
    "tslib": "2.3.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.3.5",
    "typescript-transform-paths": "^3.0.2"
  },
  "author": "JW",
  "license": "MIT",
  "scripts": {
    "build": "rimraf lib && ttsc",
    "run_dev": "rimraf lib && ttsc",
    "dev": "npm-watch run_dev",
    "test": "jest",
    "pretest": "yarn run build",
    "update": "yarn update && npm update --save-dev",
    "check": "yarn outdated && npm outdated --save-dev",
    "release": "yarn run build && np --no-cleanup --yolo --no-publish --any-branch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "gitHead": "8d1f23440ab011812cba747468cd2e4c5c36a9e4"
}
