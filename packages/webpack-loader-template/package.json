{
  "name": "@aiou/webpack-loader-template",
  "version": "0.1.7",
  "description": "a starter for webpack-loader",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "url": "https://github.com/JiangWeixian/templates/tree/master/packages/webpack-loader-template"
  },
  "keywords": [
    "webpack",
    "loader",
    "template"
  ],
  "watch": {
    "run_dev": {
      "patterns": [
        "src"
      ],
      "extensions": "ts",
      "quiet": true,
      "legacyWatch": true,
      "delay": 1000,
      "runOnChangeOnly": false
    }
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@types/jest": "26.0.24",
    "@types/loader-utils": "2.0.3",
    "@types/lodash.isempty": "4.4.6",
    "@types/schema-utils": "2.4.0",
    "husky": "3.0.1",
    "jest": "27.0.6",
    "memfs": "^3.2.2",
    "np": "7.5.0",
    "npm-watch": "0.10.0",
    "prettier": "2.3.2",
    "pretty-quick": "3.1.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.3",
    "ts-node": "10.1.0",
    "tslib": "2.3.0",
    "ttypescript": "^1.5.12",
    "typescript": "4.3.5",
    "typescript-transform-paths": "^3.0.2"
  },
  "dependencies": {
    "loader-utils": "2.0.0",
    "lodash.isempty": "4.4.0",
    "schema-utils": "3.1.0",
    "webpack": "5.44.0"
  },
  "author": "JW",
  "license": "MIT",
  "scripts": {
    "build": "rimraf lib && ttsc",
    "run_dev": "rimraf lib && ttsc",
    "dev": "npm-watch run_dev",
    "test": "jest",
    "pretest": "yarn run build",
    "update": "yarn update && yarn update --save-dev",
    "check": "yarn outdated && yarn outdated --save-dev",
    "release": "yarn run build && np --no-cleanup --yolo --no-publish --any-branch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "gitHead": "8d1f23440ab011812cba747468cd2e4c5c36a9e4"
}
